	<center style="font-size: 30px">Configure NET Screen</center>
	<form:form action="#" method="post" commandName="configureNetScreenDTO">
	<table class="tableborderspacing">
		<tr>
			<td>NetScreen Ordering Type</td>
			<td><form:select path="netScreenOrderingType"
					disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.netScreenOrderingType}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>Net Screen</td>
			<td><form:select path="netScreen" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.netScreen}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>Contract Term Year</td>
			<td><form:select path="cpeTerm" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.cpeTerm}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>Model</td>
			<td><form:input path="model" disabled="${isDisabled}" /></td>
		</tr>
		<tr>
			<td>Chassis</td>
			<td><form:input path="chassis" disabled="${isDisabled}" /></td>
		</tr>

		<tr>
			<td>Flash Memory</td>
			<td><form:select path="flashMemory" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.flashMemory}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>DRAM</td>
			<td><form:select path="dram" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.dram}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>IOS</td>
			<td><form:select path="ios" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.ios}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>Installation Type</td>
			<td><form:select path="installationType"
					disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.installationType}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>HA Type</td>
			<td><form:select path="haType" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.haType}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>USB</td>
			<td><form:select path="usb" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.usb}</option>
				</form:select></td>
		<tr>
			<td>HA License</td>
			<td><form:select path="haLicense" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.haLicense}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>CPE Owner</td>
			<td><form:select path="cpeOwner" disabled="${isDisabled}">
					<option value="">${configureNetScreenDTO.cpeOwner}</option>
				</form:select></td>
		</tr>
		<tr>
			<td>Switch Port</td>
			<td><form:input path="switchPort" disabled="${isDisabled}" /></td>
		</tr>
		<tr>
			<td>Equipment Financing</td>
			<td><form:input path="equipmentFinancing"
					disabled="${isDisabled}" /></td>
		</tr>
		
</table>
</form:form>

