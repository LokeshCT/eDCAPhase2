package com.bt.edca.common.dto;

import java.util.ArrayList;
import java.util.List;

/**
 * @author Jagankumar G.
 * DTO class for PrimarySiteContact and SecondarySiteContact lists. 
 *
 */
public class SiteContactTypesDTO {
  
  private List<SiteContactDetailsDTO> primaryContactTypeList;
  
  private List<SiteContactDetailsDTO> secondaryContactTypeList;

   /**
   * getter for PrimaryContactTypes
   * @return primaryContactTypeList.
   */
  public List<SiteContactDetailsDTO> getPrimaryContactTypeList() {
    return primaryContactTypeList;
  } 

  /**
  * setter for PrimaryContactTypes.
  * @param primaryContactTypeList.
  */
public void setPrimaryContactTypeList(List<SiteContactDetailsDTO> primaryContactTypeList) {
    this.primaryContactTypeList = primaryContactTypeList;
  }

  /**
 * @return
 */
public List<SiteContactDetailsDTO> getSecondaryContactTypeList() {
    return secondaryContactTypeList;
  }

  /**
  * setter for secondaryContactTypes.
 * @param secondaryContactTypeList.
 */
public void setSecondaryContactTypeList(List<SiteContactDetailsDTO> secondaryContactTypeList) {
    this.secondaryContactTypeList = secondaryContactTypeList;
  }
  
  
  /**
   * Constructor for SiteContactTypesDTO.
   */
  public SiteContactTypesDTO() {
    this.primaryContactTypeList = new ArrayList<SiteContactDetailsDTO>();
    this.secondaryContactTypeList = new ArrayList<SiteContactDetailsDTO>();
   
  }

  

}
