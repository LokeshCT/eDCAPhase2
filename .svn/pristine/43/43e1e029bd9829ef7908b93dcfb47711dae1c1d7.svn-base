package com.bt.edca.common.dto;

import java.util.ArrayList;
import java.util.List;

public class CustomerContactDTO {

  /**
   * variable declaration for ReutersContactList.
   */
  private List<OrderContactDTO> reutersContactList;
  /**
   * variable declaration for CommercialContactList.
   */
  private List<OrderContactDTO> commercialContactList;
  /**
   * variable declaration for TechnicalContactList.
   */
  private List<OrderContactDTO> technicalContactList;

  /**
   * Constructor for Wrapper class CustomerContactDTO.
   */
  public CustomerContactDTO() {
    this.reutersContactList = new ArrayList<OrderContactDTO>();
    this.commercialContactList = new ArrayList<OrderContactDTO>();
    this.technicalContactList = new ArrayList<OrderContactDTO>();
  }

  /**
   * gets the list of ContactType Reuters .
   * 
   * @return reutersContactList.
   */
  public List<OrderContactDTO> getReutersContactList() {
    return reutersContactList;
  }

  public void setReutersContactList(List<OrderContactDTO> reutersContactList) {
    this.reutersContactList = reutersContactList;
  }

  /**
   * gets the list of ContactType Commercial type .
   * 
   * @return commercialContactList.
   */
  public List<OrderContactDTO> getCommercialContactList() {
    return commercialContactList;
  }

  public void setCommercialContactList(List<OrderContactDTO> commercialContactList) {
    this.commercialContactList = commercialContactList;
  }

  /**
   * gets the list of ContactType Technical type .
   * 
   * @return technicalContactList.
   */
  public List<OrderContactDTO> getTechnicalContactList() {
    return technicalContactList;
  }

  public void setTechnicalContactList(List<OrderContactDTO> technicalContactList) {
    this.technicalContactList = technicalContactList;
  }

}
