package com.bt.edca.da.domain;
/**
 * Class used to Declare variables.
 * 
 * @author Mathivathana.
 * 
 */
public class TeamDomain {
	
	private int teamId;
	
	private int  teamCategoryId;
	
	private String teamName;

	/**
	 * @return the teamId
	 */
	public int getTeamId() {
		return teamId;
	}

	/**
	 * @param teamId the teamId to set
	 */
	public void setTeamId(int teamId) {
		this.teamId = teamId;
	}

	/**
	 * @return the teamCategoryId
	 */
	public int getTeamCategoryId() {
		return teamCategoryId;
	}

	/**
	 * @param teamCategoryId the teamCategoryId to set
	 */
	public void setTeamCategoryId(int teamCategoryId) {
		this.teamCategoryId = teamCategoryId;
	}

	/**
	 * @return the teamName
	 */
	public String getTeamName() {
		return teamName;
	}

	/**
	 * @param teamName the teamName to set
	 */
	public void setTeamName(String teamName) {
		this.teamName = teamName;
	}
	
	

}
