package com.bt.edca.da.domain;

/**
 * @author SATHYA.
 *
 */
public class SiteListValuesDomain {
	/**
	 * @Declaration of listValueId.
	 */
	private int listValueId;
	/**
	 * @Declaration of name.
	 */
	private String name;
	/**
	 * @Declaration of label.
	 */
	private String label;

	/**
	 * @return the listValueId
	 */
	public int getListValueId() {
		return listValueId;
	}

	/**
	 * @param listValueId
	 *            the listValueId to set
	 */
	public void setListValueId(int listValueId) {
		this.listValueId = listValueId;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name
	 *            the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return the label
	 */
	public String getLabel() {
		return label;
	}

	/**
	 * @param label
	 *            the label to set
	 */
	public void setLabel(String label) {
		this.label = label;
	}

}
